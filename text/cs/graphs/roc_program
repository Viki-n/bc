unset log
unset label
unset title
set ylabel 'Hit rate
set xlabel 'False alarm rate
set key bottom right
set size square
plot "ROCchar_data" using 1:2 title "d'=0" with lines lt rgb "black", \
"ROCchar_data" using 1:3 title "d'=1" with lines lt rgb "violet", \
"ROCchar_data" using 1:4 title "d'=2" with lines lt rgb "blue", \
"ROCchar_data" using 1:5 title "d'=3" with lines lt rgb "green", \
"ROCchar_data" using 1:6 title "d'=4" with lines lt rgb "red"
